{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "MERN app for implementing simple login, registration, form submission with authentication using JWT's",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js --ignore client",
    "client-install": "npm install --prefix client/client_app",
    "client": "npm start --prefix client/client_app",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumar306/mern-auth-form.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumar306/mern-auth-form/issues"
  },
  "homepage": "https://github.com/kumar306/mern-auth-form#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "classnames": "^2.3.1",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
